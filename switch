<?php 
//conditional  statements start
// switch statement
$a=100;
switch($a){
case 50:
    echo"the number is 50";
    break;
case 100:
    // can use print replace of echo it gives output
    print"the number is 100";
    break;
    // suppose if u give any other values of none of the above it excutes default case
default:
echo "other value";
}

?>
